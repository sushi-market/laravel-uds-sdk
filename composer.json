    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.10|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SushiMarket\\UdsSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SushiMarket\\UdsSdk\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SushiMarket\\UdsSdk\\UdsIntegrationProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
